        <TextBlock HorizontalAlignment="Left" Margin="33,230,0,0" TextWrapping="Wrap" Foreground="Black" FontSize="20" Width="158" RenderTransformOrigin="0.509,-0.096" Height="34" VerticalAlignment="Top" Text="Password"/>
        <Button Content="Sign Up" HorizontalAlignment="Left" Margin="56,421,0,0" VerticalAlignment="Top" Width="232" Height="33" Background="#FF164283" Foreground="White" BorderThickness="0" BorderBrush="White" Click="Button_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="33,389,0,0" TextWrapping="Wrap" Foreground="Black" Width="208" RenderTransformOrigin="0.509,-0.096" Height="27" VerticalAlignment="Top" Text="Don't have an account ?" FontSize="16"/>
